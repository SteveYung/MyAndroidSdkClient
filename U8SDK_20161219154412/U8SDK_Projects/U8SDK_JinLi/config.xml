<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params>
        <param name="JinLi_ApiKey" required="1" showName="ApiKey" desc="金立" bWriteInManifest="0" bWriteInClient="1" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.JinLiUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.JinLiPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>金立</name>
        <versionCode>1</versionCode>
        <versionName>3.1.9.i</versionName>
    </version>    
    
</config>